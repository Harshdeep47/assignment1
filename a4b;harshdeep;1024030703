#include <iostream>
using namespace std;

int main() {
    int a[10][10], b[10][10], c[10][10], n, m, k, p;
    cout << "Enter rows and cols for first matrix: ";
    cin >> n >> m;
    cout << "Enter rows and cols for second matrix: ";
    cin >> k >> p;
    if (m != k) {
        cout << "Matrix multiplication not possible\n";
        return 1;
    }
    cout << "Enter first matrix:\n";
    for (int i = 0; i < n; ++i)
        for (int j = 0; j < m; ++j)
            cin >> a[i][j];
    cout << "Enter second matrix:\n";
    for (int i = 0; i < k; ++i)
        for (int j = 0; j < p; ++j)
            cin >> b[i][j];
    for (int i = 0; i < n; ++i)
        for (int j = 0; j < p; ++j) {
            c[i][j] = 0;
            for (int l = 0; l < m; ++l)
                c[i][j] += a[i][l] * b[l][j];
        }
    cout << "Product:\n";
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < p; ++j)
            cout << c[i][j] << " ";
        cout << endl;
    }
    return 0;
}
